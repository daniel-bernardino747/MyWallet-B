{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": ["warn", {"js": "always"}],
        "import/no-cycle": [2, {"maxDepth": 1}],
        "no-underscore-dangle": ["error", {"allow": ["_id"]}]
    }
}
